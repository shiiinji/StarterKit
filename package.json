{
  "name": "typescript-next.js-node.js-starter-kit",
  "version": "0.0.1",
  "description": "A starting point for Node.js express next.js react redux PWA jest apps with TypeScript",
  "main": "server.js",
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "tsc": "tsc",
    "dev": "node server.js",
    "build": "next build app",
    "watch": "tsc --watch",
    "start": "NODE_ENV=production node server.js",
    "debug": "NODE_ENV=production node --inspect server.js",
    "test": "jest --config jest.config.js",
    "test:watch": "npm test -- --watch",
    "lint:server": "tslint -c tslint.json -t stylish -p ./tsconfig.json",
    "lint:client": "tslint -c tslint.json -t stylish -p ./app/tsconfig.json",
    "lint:precommit": "tslint -c tslint.json ./",
    "lint": "npm-run-all lint:server lint:client",
    "prettier": "prettier --write --config .prettierrc.js",
    "security-check": "nsp check"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/Shinji_Teramoto/starterkit.git"
  },
  "author": "shinji <shinji.toys@gmail.com>",
  "license": "MIT",
  "lint-staged": {
    "*.{ts, tsx}": [
      "npm run prettier",
      "npm run lint:precommit",
      "git add"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.236.1",
    "axios": "^0.17.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cron": "^1.2.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "express-rate-limit": "^2.11.0",
    "express-react-views": "^0.10.2",
    "express-session": "^1.15.4",
    "helmet": "^3.12.0",
    "husky": "^0.14.3",
    "import-export": "^1.0.1",
    "lint-staged": "^6.0.0",
    "log4js": "^2.5.3",
    "method-override": "^2.3.9",
    "mime": "^1.3.6",
    "moment": "^2.22.0",
    "mongoose": "^5.0.14",
    "multer": "^1.3.0",
    "next": "6.0.2",
    "next-routes": "^1.4.1",
    "node-mocks-http": "^1.5.8",
    "numeral": "^2.0.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "request": "^2.85.0"
